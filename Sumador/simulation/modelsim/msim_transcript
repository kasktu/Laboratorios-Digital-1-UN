# Usage: wave create -driver <freeze|deposit|drive|expectedOutput> -pattern constant -value <value> [-range <msb lsb>] -starttime <Time> -endtime <Time> netName
wave create -driver freeze -pattern random -initialvalue 0 -period 50ps -random_type Uniform -seed 1 -starttime 0ps -endtime 1000ps sim:/sumador/a1
wave create -driver freeze -pattern random -initialvalue 0 -period 50ps -random_type Uniform -seed 2 -starttime 0ps -endtime 1000ps sim:/sumador/a2
wave create -driver freeze -pattern repeater -initialvalue 0 -period 50ps -sequence { 1 0  } -repeat forever -starttime 0ps -endtime 1000ps sim:/sumador/a3
wave create -driver freeze -pattern constant -value 1 -starttime 0ps -endtime 1000ps sim:/sumador/b1
wave create -driver freeze -pattern constant -value 1 -starttime 0ps -endtime 1000ps sim:/sumador/b2
wave create -driver freeze -pattern repeater -initialvalue 0 -period 50ps -sequence { 0 1  } -repeat forever -starttime 0ps -endtime 1000ps sim:/sumador/b3
wave create -driver freeze -pattern repeater -initialvalue 0 -period 100ps -sequence { 1 0  } -repeat forever -starttime 0ps -endtime 1000ps sim:/sumador/SR
add wave -position end  sim:/sumador/Sout1
add wave -position end  sim:/sumador/Sout2
add wave -position end  sim:/sumador/Sout3
add wave -position end  sim:/sumador/Sout4
run -all
